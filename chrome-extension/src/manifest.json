{
  "name": "Better Bookmarks",
  "description" : "Type 'bb' plus a bookmark key into the Omnibox.",
  "version": "1.0",
  "background": {
    "scripts": ["bundle.js"]
  },
  "omnibox": { "keyword" : "bb" },
  "browser_action": {
    "default_icon": "bb-icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "notifications"
  ],
  "manifest_version": 2
}
